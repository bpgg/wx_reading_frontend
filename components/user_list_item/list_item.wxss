/* components/user_list_item/list_item.wxss */
.list-item{
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  line-height: 80rpx;
  box-sizing: border-box;
  padding: 10rpx 0;
}
.list-item-left{
  font-size: 30rpx;
}
.list-item-right{
  display: flex;
  flex-direction: column;
  font-size: 40rpx;
  font-weight: 800;
}
