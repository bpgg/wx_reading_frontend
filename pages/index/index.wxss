/**index.wxss**/
page{
  background-color: #f1f1f1
}
.search-box{
  width: 90%;
  position: relative;
  margin: 20rpx auto;
}
.search-box image{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  left: 15rpx;
  top: 50%;
  transform: translateY(-50%)
}
.search-box input{
  width: 100%;
  box-sizing: border-box;
  padding-left: 60rpx;
  font-size: 28rpx;
  height: 65rpx;
  outline: none;
  background-color: #f9f9f9;
  border-radius: 30rpx;
  caret-color:#028ffa;
}
@import "./tpl_swiper/swiper.wxss";
